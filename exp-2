num=int(input("enter a number:"))
if num>=1 and num<=100:
    print("the number",num,"is not b/w number")
    

N=int(input("Enter the value of N:"))
count=0
num=5
while count<N:
    print(num)
    num+=5
    count+=1
    

num=0
while num<=20:
    if num%2==0:
        print(num,end= ",")
    num+=2


