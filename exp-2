num=int(input("Enter a number:"))
if num==0:
    print("zero")
elif num==1:
    print("one")
elif num==2:
    print("two")
elif num==3:
    print("three")
elif num==4:
    print("four")
elif num==5:
    print("five")
elif num==6:
    print("six")
elif num==7:
    print("seven")
elif num==8:
    print("eight")
elif num==9:
    print("nine")
else:
    print("number out of range")


for i in range(1,11):
    if i==5:
        continue 
    if i==8:
        break
print(i)


name=input("Enter your name:")
age=int(input("Enter your age:"))
if age>=18:
    print("Congratulation",name+ "your age eligible to vote")
